{
  "name": "tenancy-server",
  "version": "1.0.0",
  "description": "Backend Server for Tenancy",
  "main": "server.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "nodemon server.ts",
    "encryptkey:generate": "tsc generateAppKey.ts && node generateAppKey.js",
    "migration:generate": "npx drizzle-kit generate",
    "migration:up": "npx drizzle-kit up",
    "migration:down": "npx drizzle-kit down",
    "migration:drop": "npx drizzle-kit drop",
    "migration:status": "npx drizzle-kit status",
    "migration:apply": "npx drizzle-kit apply",
    "migration:push": "ts-node drizzle.migrate.ts",
    "migration:rollback": "npx drizzle-kit rollback",
    "db:seed": "tsc database/seeds/index.seed.ts && node dist/database/seeds/index.seed.js",
    "db:reset": "npx drizzle-kit reset"
  },
  "author": "Bada Adeola",
  "license": "MIT",
  "dependencies": {
    "ably": "^2.3.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bullmq": "^5.10.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.32.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "express-validator": "^7.1.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "mongoose": "^8.5.3",
    "mysql2": "^3.10.3",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.14",
    "nodemon": "^3.1.4",
    "pg": "^8.12.0",
    "redis": "^4.6.15",
    "socket.io": "^4.7.5",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.6",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.12.0",
    "drizzle-kit": "^0.23.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
